{
  "name": "jdwx/acme",
  "autoload": {
    "psr-4": {
      "JDWX\\ACME\\": "src/"
    }
  },
  "bin": [
    "bin/acme-cli.php"
  ],
  "require": {
    "php": ">=8.3.0",
    "ext-curl": "*",
    "ext-openssl": "*",
    "jdwx/cli": "^1.0",
    "jdwx/config": "^1.0",
    "jdwx/json-api-client": "^1.0",
    "jdwx/param": "^1.0",
    "jdwx/result": "^v1.0",
    "jdwx/strict": "^1.0.23",
    "web-token/jwt-library": "^3.4"
  },
  "require-dev": {
    "jetbrains/phpstorm-attributes": "^1.0",
    "roave/security-advisories": "dev-latest",
    "phan/phan": "^v5",
    "phpstan/phpstan": "^2.0",
    "phpunit/phpunit": "^12"
  },
  "scripts": {
    "phan": "PHAN_ALLOW_XDEBUG=1 php -d xdebug.mode=off vendor/bin/phan --analyze-twice",
    "phpstan": "phpstan",
    "test": "vendor/bin/phpunit tests"
  }
}
